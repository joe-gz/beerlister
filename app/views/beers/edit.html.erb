<div class = "wrapper">
  <h2 class = "edit_beer">Edit <%=@beer.name%> </h2>
  <!-- standard form_for to edit the review on a given beer
  Only the user who posted the beer can edit -->
  <%= form_for @beer do |f| %>

  <%= f.label :review %>
  <%= f.text_field :review %>

  <%= f.submit class:"submit"%>

  <%end%>
</div>
